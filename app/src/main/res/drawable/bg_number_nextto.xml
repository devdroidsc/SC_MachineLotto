<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape >
            <gradient android:angle="90"
                android:startColor="#01649f"
                android:endColor="#01649f"></gradient>
            <stroke android:width="@dimen/magin2"
                android:color="#000000"></stroke>
            <corners android:radius="@dimen/magin10"/>
        </shape>
    </item>
    <item>
        <shape >
            <gradient android:angle="90"
                android:startColor="#0277BD"
                android:endColor="#0277BD"></gradient>
            <stroke android:width="@dimen/magin2"
                android:color="#000000"></stroke>
            <corners android:radius="@dimen/magin10"/>
        </shape>
    </item>


</selector>
