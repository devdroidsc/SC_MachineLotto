<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >
            <gradient android:angle="90"
                android:startColor="#962a09"
                android:endColor="#962a09"></gradient>
            <stroke android:width="@dimen/magin2"
                android:color="#000000"></stroke>
            <corners android:radius="@dimen/magin10"/>

        </shape>
    </item>
    <item>
        <shape >
            <gradient android:angle="90"
                android:startColor="#BF360C"
                android:endColor="#BF360C"></gradient>
            <stroke android:width="@dimen/magin2"
                android:color="#000000"></stroke>
            <corners android:radius="@dimen/magin10"/>

        </shape>
    </item>
</selector>
